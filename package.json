{
  "name": "atomic-calendar",
  "version": "1.0.0",
  "description": "Calendar Card for Home Assistant",
  "main": "atomic_calendar.js",
  "scripts": {
    "start": "npm-run-all -p serve watch",
    "lint": "eslint app.js",
    "babel": "babel atomic_calendar.js --out-file atomic_calendar.js",
    "rollup": "rollup -c",
    "build": "npm run rollup  && npm run babel",
    "watch": "rollup -c --watch --config rollup-dev.config.js",
    "serve": "ngrok http 5000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atomic7777/atomic_calendar.git"
  },
  "author": "Wojciech Mamak",
  "license": "ISC",
  "dependencies": {
    "lit-element": "2.0.1",
    "moment": "2.24.0",
    "node-google-calendar": "1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "@babel/plugin-transform-template-literals": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "babel-loader": "8.0.5",
    "babel-preset-minify": "0.5.0",
    "eslint": "5.14.1",
    "ngrok": "3.2.7",
    "npm-run-all": "4.1.5",
    "rollup": "1.26.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-serve": "1.0.1",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "4.0.4",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "bugs": {
    "url": "https://github.com/atomic7777/atomic_calendar/issues"
  },
  "homepage": "https://github.com/atomic7777/atomic_calendar#readme"
}
